<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://traffic.nokia.com/trafficml-flow-3.2" targetNamespace="http://traffic.nokia.com/trafficml-flow-3.2" elementFormDefault="qualified" version="3.2.1.1">
   <xs:element name="TRAFFICML_REALTIME">
      <xs:annotation>
         <xs:documentation>The general structure of the TrafficML feed contains the root element,				TRAFFICML_REALTIME, which contains zero to many instances of RWS (ROADWAYS). RWS				contains 1 to many instances of RW (ROADWAY) Roadway locations can be addressed by				one of two methods: "TMC", "SHP". TMC locations us TMC Location number from TMC				Tables. SHP use a directed set of Lat/Long pairs.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element name="FEATURES">
               <xs:annotation>
                  <xs:documentation>Indicates which optional features are present in this file.</xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:sequence>
                     <xs:element name="FEATURE" type="feature" minOccurs="0" maxOccurs="unbounded" />
                  </xs:sequence>
               </xs:complexType>
            </xs:element>
            <xs:element name="RWS" type="RWS_TYPE" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>A list of Roadway (RW) items.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="diagnostic" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>Optional information used internally by Nokia. This must							be ignored by customers.</xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:sequence>
                     <xs:element name="info" type="xs:string" minOccurs="0" />
                  </xs:sequence>
                  <xs:attribute name="traceId" type="xs:string" use="optional" />
                  <xs:attribute name="sfile" type="xs:string" use="optional" />
                  <xs:attribute name="pdd" type="xs:string" use="optional" />
               </xs:complexType>
            </xs:element>
         </xs:sequence>
         <xs:attribute name="MAP_VERSION" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>NAVTEQ map version used to create locations in this file. e.g.						201201</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="TMC_TABLE_VERSION" type="xs:string" use="optional">
            <xs:annotation>
               <xs:documentation>TMC Table version. In some cases these value may not be available.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="CREATED_TIMESTAMP" type="xs:dateTime" use="required">
            <xs:annotation>
               <xs:documentation>When this file was created in UTC. Syntax described in						http://www.w3.org/TR/xmlschema-2/#dateTime e.g. 2010-05-18T01:00:00Z</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="VERSION" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>TRAFFICML product version e.g. 3.2</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="UNITS" type="UNIT_TYPE" use="required">
            <xs:annotation>
               <xs:documentation>Units of measurements used for flow items</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:complexType name="RWS_TYPE">
      <xs:annotation>
         <xs:documentation>Linears (Roadways) use TMC addressing</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="RW" type="RW_TYPE" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>This is the composite item for flow across an entire roadway.						A roadway item will be present for each roadway with traffic flow						information available.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="TY" type="xs:string" use="required">
         <xs:annotation>
            <xs:documentation>TMC for TMC roadways/linears. SHP element will usually not be					present.</xs:documentation>
            <xs:documentation>SHP for shape points. The TMC element will not usually be					present.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="MAP_VERSION" type="xs:string" use="optional">
         <xs:annotation>
            <xs:documentation>NAVTEQ map version used to create locations in this file. e.g.					201201</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="EBU_COUNTRY_CODE" type="xs:string" use="required">
         <xs:annotation>
            <xs:documentation>TMC Table country code - e.g. US = 1, Germany =D</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="EXTENDED_COUNTRY_CODE" type="xs:string" use="required">
         <xs:annotation>
            <xs:documentation>e.g. A0 = US</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="TABLE_ID" type="xs:string" use="required">
         <xs:annotation>
            <xs:documentation>TMC Table - e.g. US/Chicago area = 7, Germany =1</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="TMC_TABLE_VERSION" type="xs:string" use="optional">
         <xs:annotation>
            <xs:documentation>TMC Table version</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="UNITS" type="UNIT_TYPE" use="optional">
         <xs:annotation>
            <xs:documentation>Units of measurements used for flow items in this RWS					element</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="RW_TYPE">
      <xs:sequence>
         <xs:element name="FIS" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>A list of Flow Item (FI) elements</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="FI" type="FI_TYPE" maxOccurs="unbounded">
                     <xs:annotation>
                        <xs:documentation>A single flow item.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="LI" type="xs:string" use="optional">
         <xs:annotation>
            <xs:documentation>Unique string identifier for this Linear</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="DE" type="xs:string">
         <xs:annotation>
            <xs:documentation>Text description of the road.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="PBT" type="xs:string">
         <xs:annotation>
            <xs:documentation>Basetimestamp used as a reference for all predictive cacluluations in this Roadway</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="mid" type="xs:string" use="optional">
         <xs:annotation>
            <xs:documentation>Nokia internal identifier. Must not be used by customers. Contents may vary without notice.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="FI_TYPE">
      <xs:sequence>
         <xs:choice>
            <xs:element name="TMC" type="TMC_LR_TYPE" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>An ordered collection of TMC locations.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="GENERIC_DLR" type="GENERIC_LR_CONTAINER_TYPE" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>A generic container for future location referencing							formats</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
         <xs:element name="SHP" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>A polyline defined as space-seperated pairs of comma-seperated						lat/longs. e.g. 37.71508,-122.39795 37.71463,-122.39759</xs:documentation>
               <xs:documentation>There must be at least two pairs.</xs:documentation>
               <xs:documentation>Direction is indicated by the order of the lat/longpairs in						the polyline.</xs:documentation>
               <xs:documentation>Only present if @TY is "SHP" when generated by Halo.						Applications may add the SHP element on an ad-hoc basis.</xs:documentation>
               <xs:documentation>Since v.3.2.1 will only have more than one SHP element if DLR_AGGREGATION is in FEATURES.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:simpleContent>
                  <xs:extension base="xs:string">
                     <xs:attribute name="FC" type="functionalClass" use="optional">
                        <xs:annotation>
                           <xs:documentation>Functional class.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="LID" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Link ID and direction (pvid from the HERE										map.) e.g. 123456789T or 987654321F. If more than one pvid,										they are separated by commas.</xs:documentation>
                           <xs:documentation>Since v3.1.2</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="LE" type="length" use="optional">
                        <xs:annotation>
                           <xs:documentation>Length of the stretch of road. The units are defined in the file header.</xs:documentation>
                           <xs:documentation>Since v3.2.0</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="FW" type="formOfWay" use="optional">
                        <xs:annotation>
                           <xs:documentation>Form of way</xs:documentation>
                           <xs:documentation>Since v3.2.0</xs:documentation>
                           <xs:documentation>Since v3.2.1 only present if FORM_OF_WAY is in FEATURES.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:simpleContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="CF" type="CF_TYPE" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>Current Flow. This element contains details about speed and						Jam Factor information for the given flow item.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PF" type="PF_TYPE" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Predictive Flow. This element contains details about predicted						speed values for epochs in the future. Note that this element is optional and well not contain data for the first releases.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="TMC_LR_TYPE">
      <xs:attribute name="PC" type="xs:int">
         <xs:annotation>
            <xs:documentation>Point TMC Location Code</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="DE" type="xs:string" use="required">
         <xs:annotation>
            <xs:documentation>Description for the given location.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="QD" type="xs:string" use="required">
         <xs:annotation>
            <xs:documentation>Queuing direction. '+' or '-'. Note this is the opposite of the travel direction in the fully qualified ID, For example for location 107+03021 the QD would be '-'.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="LE" type="length" use="required">
         <xs:annotation>
            <xs:documentation>Length of item (based on UNITS)</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="FC" type="functionalClass" use="optional">
         <xs:annotation>
            <xs:documentation>Functional class. Usually not present.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="SN" type="xs:string" use="optional">
         <xs:annotation>
            <xs:documentation>Indicates when data has changed. Usually not					present.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="CF_TYPE">
      <xs:sequence>
         <xs:element name="SSS" type="SSS_TYPE" minOccurs="0" />
         <xs:element name="LN" type="LN_TYPE" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>Describes traffic conditions for all the lanes in a						"split-lane" situation. This can occur either in SSS or CF_TYPE but not both.</xs:documentation>
               <xs:documentation>Since v3.2.0</xs:documentation>
               <xs:documentation>Since v3.2.1 only present if LANES is in FEATURES</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="TY" type="laneType" use="optional">
         <xs:annotation>
            <xs:documentation>Used when it is needed to differentiate between different kinds of					location types.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="SP" type="speed" use="required">
         <xs:annotation>
            <xs:documentation>Speed (based on UNITS) capped by speed limit</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="SU" type="speed" use="optional">
         <xs:annotation>
            <xs:documentation>Speed (based on UNITS) not capped by speed					limit</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="FF" type="speed" use="required">
         <xs:annotation>
            <xs:documentation>The free flow speed on this stretch of road.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="JF" type="jamFactor" use="required">
         <xs:annotation>
            <xs:documentation>The number between 0.0 and 10.0 indicating the expected quality of travel. When there is a road closure, the Jam Factor will be 10. As the number					approaches 10.0 the quality of travel is getting worse. -1.0 indicates that a					Jam Factor could not be calculated.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="CN" use="required">
         <xs:annotation>
            <xs:documentation>Confidence, an indication of how the speed was determined. -1.0					road closed. 1.0=100% 0.7-100% Historical Usually a value between .7 and 1.0.</xs:documentation>
         </xs:annotation>
         <xs:simpleType>
            <xs:restriction base="xs:double">
               <xs:minInclusive value="-1" />
               <xs:maxInclusive value="1.0" />
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="TS" type="traversabilityStatus" use="optional">
         <xs:annotation>
            <xs:documentation>Traversability Status</xs:documentation>
            <xs:documentation>Since v3.1.2</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="SSS_TYPE">
      <xs:sequence>
         <xs:element name="SS" type="SS_TYPE" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>Only provided if there is a current traffic data of						SubSegments is different than the current traffic data of the FlowItem.						Refer to the SS_TYPE for details of the SS element.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="SS_TYPE">
      <xs:annotation>
         <xs:documentation>The SUB_SEGMENT (SS_TYPE) will be provided if the current traffic data				is more precise than the TMC boundary. When there are SS elements, the elements will				be ordered by driving direction. If there are SS elements, the LENGTH (LE) of all				SUB_SEGMENT elements will add up to the LENGTH of the roadway represented from the				corresponding CURRENT_FLOW (CF) section.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="LN" type="LN_TYPE" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>Describes traffic conditions for all the lanes in a						"split-lane" situation.  This can occur either in SSS or CF_TYPE but not both.</xs:documentation>
               <xs:documentation>Since v3.2.0</xs:documentation>
               <xs:documentation>Since v3.2.1 only present if LANES is in FEATURES</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="LE" type="length" use="required">
         <xs:annotation>
            <xs:documentation>Length of the stretch of road. The units are defined in the file					header.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="SP" type="speed" use="required">
         <xs:annotation>
            <xs:documentation>Speed (based on UNITS) capped by speed limit</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="SU" type="speed" use="optional">
         <xs:annotation>
            <xs:documentation>Speed (based on UNITS) not capped by speed limit</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="FF" type="speed" use="required">
         <xs:annotation>
            <xs:documentation>Free flow speed.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="JF" type="jamFactor" use="required">
         <xs:annotation>
            <xs:documentation>Jam factor 0.0 (free flowing) to 10.0 (road closed).</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="TS" type="traversabilityStatus" use="optional">
         <xs:annotation>
            <xs:documentation>Traversability Status</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="LN_TYPE">
      <xs:annotation>
         <xs:documentation>Lane Element.</xs:documentation>
         <xs:documentation>Since v3.2.0</xs:documentation>
      </xs:annotation>
      <xs:attribute name="NM" type="lanes" use="required" />
      <xs:attribute name="SU" type="speed" use="required" />
      <xs:attribute name="JF" type="jamFactor" use="required" />
   </xs:complexType>
   <xs:simpleType name="lanes">
      <xs:annotation>
         <xs:documentation>Comma delimited list of lanes associated with the traffic. The lane				numbers are referenced from the HERE Map (Left Lane = 1, Right Lane =				x)</xs:documentation>
         <xs:documentation>All lanes from 1 to N must be included, when all LN elements are				considered.</xs:documentation>
         <xs:documentation>Since v3.2.0</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string" />
   </xs:simpleType>
   <xs:complexType name="PF_TYPE">
      <xs:annotation>
         <xs:documentation>Predictive</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="P" type="P_TYPE" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="P_TYPE">
      <xs:attribute name="UT" type="xs:int" use="required">
         <xs:annotation>
            <xs:documentation>Time in minutes that prediction is effective until. This time is					the number of minutes beyond the time that is defined in the basetime attribute.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="SP" type="predictiveSpeed" use="required">
         <xs:annotation>
            <xs:documentation>Predicted speed in mph.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:simpleType name="jamFactor">
      <xs:restriction base="xs:double">
         <xs:minInclusive value="-1" />
         <xs:maxInclusive value="10.0" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="speed">
      <xs:restriction base="xs:double" />
   </xs:simpleType>
   <xs:simpleType name="predictiveSpeed">
      <xs:restriction base="xs:int" />
   </xs:simpleType>
   <xs:simpleType name="length">
      <xs:restriction base="xs:double" />
   </xs:simpleType>
   <xs:simpleType name="functionalClass">
      <xs:restriction base="xs:int">
         <xs:minInclusive value="1" />
         <xs:maxInclusive value="5" />
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="traversabilityStatus">
      <xs:annotation>
         <xs:documentation>Since v3.1.2</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="O">
            <xs:annotation>
               <xs:documentation>Open, drivers can enter this piece of						roadway.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="C">
            <xs:annotation>
               <xs:documentation>Closed, based on an observed or scheduled road						closure.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RNR">
            <xs:annotation>
               <xs:documentation>Reversible Not Routable. A reversible road that is not open in						this direction.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="formOfWay">
      <xs:annotation>
         <xs:documentation>Form of way.</xs:documentation>
         <xs:documentation>Since v3.2.0</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="MW">
            <xs:annotation>
               <xs:documentation>Motorway.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MD">
            <xs:annotation>
               <xs:documentation>Multi-digitized road which is not a						motorway.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SD">
            <xs:annotation>
               <xs:documentation>Single digitized road.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RA">
            <xs:annotation>
               <xs:documentation>Ramp/Slip.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RO">
            <xs:annotation>
               <xs:documentation>Roundabout.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="UN">
            <xs:annotation>
               <xs:documentation>Undefined.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="UNIT_TYPE">
      <xs:restriction base="xs:string">
         <xs:enumeration value="metric">
            <xs:annotation>
               <xs:documentation>Speeds in KPH, distance in Kilometers</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="imperial">
            <xs:annotation>
               <xs:documentation>Speeds in MPH, distance in Miles</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType name="GENERIC_LR_CONTAINER_TYPE">
      <xs:simpleContent>
         <xs:extension base="xs:string">
            <xs:attribute name="TY" type="xs:string">
               <xs:annotation>
                  <xs:documentation>Type information for the given Location Referencing							container. This may be freely defined string</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="VER" type="xs:string">
               <xs:annotation>
                  <xs:documentation>Version of the specified location							referencing</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
   <xs:simpleType name="feature">
      <xs:annotation>
         <xs:documentation>Since v3.2.1</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="LANES">
            <xs:annotation>
               <xs:documentation>Lane level information. In SS_TYPE/LN element.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="EXPRESS">
            <xs:annotation>
               <xs:documentation>Indicates that the EXPRESS (EX) value for CF@TY may occur.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FORM_OF_WAY">
            <xs:annotation>
               <xs:documentation>Indicates the SHP@FW attribute may be present.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DLR_AGGREGATION">
            <xs:annotation>
               <xs:documentation>Indicates there may be multiple SHP elements in an FI element.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="laneType">
      <xs:annotation>
         <xs:documentation>Changed from simple string to enumeration in v3.2.1</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="TR">
            <xs:annotation>
               <xs:documentation>Normal traffic lane.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RA">
            <xs:annotation>
               <xs:documentation>Ramp.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="EX">
            <xs:annotation>
               <xs:documentation>Express lanes are called out separately to enable different usage in certain applications, such as map display.  Express lanes may or may not be reversible.</xs:documentation>
